{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:testnet",
    "dev:localnet": "concurrently \"vite --mode localnet\" \"npm run tunnel\"",
    "dev:testnet": "concurrently \"vite --mode testnet\" \"npm run tunnel\"",
    "dev:mainnet": "concurrently \"vite --mode mainnet\" \"npm run tunnel\"",
    "tunnel": "cloudflared tunnel run shreddr-dev",
    "build": "tsc -b && vite build",
    "build:testnet": "tsc -b && vite build --mode testnet",
    "build:mainnet": "tsc -b && vite build --mode mainnet",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.10.2",
    "@coinflowlabs/react": "^5.5.1",
    "@connectrpc/connect": "^2.1.1",
    "@connectrpc/connect-web": "^2.1.1",
    "@phantom/react-sdk": "^1.0.0",
    "@solana/web3.js": "^1.98.4",
    "@tanstack/react-router": "^1.143.11",
    "@tanstack/react-router-devtools": "^1.143.11",
    "bs58": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql": "^16.12.0",
    "lucide-react": "^0.562.0",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.61.0",
    "@bufbuild/protoc-gen-es": "^2.10.2",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/router-plugin": "^1.143.11",
    "@types/node": "^24.10.1",
    "@types/qrcode.react": "^1.0.5",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
